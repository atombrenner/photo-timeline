{
  "name": "photo-timeline",
  "repository": "",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "SNOWPACK_PUBLIC_MEDIA_URL=http://localhost:9000 snowpack dev",
    "build": "SNOWPACK_PUBLIC_MEDIA_URL=/Photos snowpack build",
    "deploy": "npm run build && cp -r build/* ~/Data/MyMedia/",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx}\"",
    "serve": "nodemon -e ts -w backend -x 'ts-node -T -P backend/tsconfig.json backend/index.ts'",
    "ingest": "ts-node -T -P scripts/tsconfig.json scripts/ingest",
    "ingest:camera": "ts-node -T -P scripts/tsconfig.json scripts/ingest /run/media/christian/9016-4EF8/DCIM/",
    "reindex": "ts-node -T -P scripts/tsconfig.json scripts/ingest reindex",
    "ffprobe": "ts-node -T -P scripts/tsconfig.json scripts/ffprobe",
    "repair": "ts-node -T -P scripts/tsconfig.json scripts/repair"
  },
  "dependencies": {
    "date-fns": "^2.21.1",
    "exif-reader": "^1.0.3",
    "fastify": "^3.15.1",
    "fastify-static": "^4.0.1",
    "fs-extra": "^10.0.0",
    "preact": "^10.5.13",
    "sharp": "^0.28.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@prefresh/snowpack": "^3.1.2",
    "@snowpack/plugin-typescript": "^1.2.0",
    "@testing-library/preact": "^2.0.0",
    "@trivago/prettier-plugin-sort-imports": "^2.0.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/sharp": "^0.28.1",
    "babel-jest": "^26.6.3",
    "http-server": "^0.12.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "snowpack": "^3.3.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
