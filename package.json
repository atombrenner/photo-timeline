{
  "name": "photo-timeline",
  "repository": "",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "SNOWPACK_PUBLIC_MEDIA_URL=http://localhost:9000 snowpack dev",
    "build": "SNOWPACK_PUBLIC_MEDIA_URL=/Photos snowpack build",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx}\"",
    "serve": "http-server ~/Data/MyMedia/Photos --cors --silent -p 9000",
    "prod": "http-server ~/Data/MyMedia --silent -p 3000",
    "ingest": "ts-node -T -P scripts/tsconfig.json scripts/ingest",
    "index": "ts-node -T -P scripts/tsconfig.json scripts/create-index",
    "ffprobe": "ts-node -T -P scripts/tsconfig.json scripts/ffprobe",
    "repair": "ts-node -T -P scripts/tsconfig.json scripts/repair"
  },
  "dependencies": {
    "date-fns": "^2.19.0",
    "exif-reader": "^1.0.3",
    "fs-extra": "^9.1.0",
    "http-server": "^0.12.3",
    "preact": "^10.5.13",
    "sharp": "^0.27.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@prefresh/snowpack": "^3.1.2",
    "@snowpack/plugin-typescript": "^1.2.0",
    "@testing-library/preact": "^2.0.0",
    "@trivago/prettier-plugin-sort-imports": "^2.0.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.21",
    "@types/sharp": "^0.27.1",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "snowpack": "^3.1.2",
    "tailwindcss": "^2.0.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
